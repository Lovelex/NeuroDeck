{
  "name": "neurodeck",
  "private": true,
  "version": "0.1.0",
  "description": "Active learning through interruptions",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "npm run build:electron && concurrently -k \"vite\" \"npm run dev:electron\"",
    "dev:electron": "wait-on http://127.0.0.1:5173 && cross-env NODE_ENV=development electron .",
    "build": "tsc && vite build && npm run build:electron",
    "build:electron": "tsc -p electron/tsconfig.json",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "vitest run",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.53.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.17",
    "wait-on": "^7.2.0"
  }
}
